{
  "name": "adlogix/zf-symfony-container",
  "description": "Zend Framework module to leverage the symfony dependency injection container",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "zf2",
    "symfony",
    "dependency injection",
    "container"
  ],
  "homepage": "https://github.com/adlogix/zf-symfony-container",
  "authors": [
    {
      "name": "Toni Van de Voorde",
      "email": "toni.vdv@gmail.com",
      "homepage": "http://www.devexp.eu"
    }
  ],
  "require": {
    "php": ">=5.6",
    "zendframework/zend-modulemanager": "^2.8",
    "zendframework/zend-servicemanager": "^2.7",
    "symfony/dependency-injection": "^3.4",
    "symfony/config": "^3.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.0",
    "zendframework/zend-i18n": "^2.8",
    "zendframework/zend-log": "^2.10",
    "zendframework/zend-mvc": "^2.7",
    "zendframework/zend-serializer": "^2.9",
    "zendframework/zend-test": "^2.6",
    "friendsofphp/php-cs-fixer": "^2.13"
  },
  "autoload": {
    "psr-4": {
      "Adlogix\\ZfSymfonyContainer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Adlogix\\ZfSymfonyContainer\\Test\\": "tests/"
    }
  }
}
